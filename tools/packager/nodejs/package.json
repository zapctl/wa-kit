{
  "name": "wa-kitchen",
  "description": "WhatsApp Web Protocol Buffers",
  "version": "{{WA_VERSION}}",
  "private": false,
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./graphql": {
      "types": "./graphql/index.d.ts",
      "default": "./graphql/index.js"
    },
    "./proto/*": {
      "types": "./proto/*_pb.d.ts",
      "default": "./proto/*_pb.js"
    },
    "./binary/*": {
      "types": "./binary/*.d.ts",
      "default": "./binary/*.js"
    },
    "./jid/*": {
      "types": "./jid/*.d.ts",
      "default": "./jid/*.js"
    },
    "./message/*": {
      "types": "./message/*.d.ts",
      "default": "./message/*.js"
    }
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "keywords": [
    "whatsapp",
    "protobuf",
    "graphql",
    "typescript"
  ],
  "license": "MIT",
  "author": "Jo√£o Vitor Machado <joaov@zapctl.dev>",
  "homepage": "https://github.com/zapctl/wa-kitchen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zapctl/wa-kitchen.git"
  },
  "bugs": {
    "url": "https://github.com/zapctl/wa-kitchen/issues"
  },
  "dependencies": {
    "@bufbuild/protobuf": "1.10.0"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "1.10.0",
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "uglify-js": "^3.19.3"
  }
}